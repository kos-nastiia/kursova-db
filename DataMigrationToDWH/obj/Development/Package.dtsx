                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    sa.student_id,
    sa.group_id,
    sa.average_grade AS rating,
    CASE  
        WHEN ts.rank &lt;= 0.35 THEN 1000.00
        ELSE 0.00
    END AS scholarship_sum,
    sa.year
FROM (
    SELECT 
        s.student_id,
        AVG(g.points) AS average_grade,
        sg.group_id,
        gr.year
    FROM grades g
    JOIN student s ON g.student_id = s.student_id
    JOIN student_group sg ON s.student_id = sg.student_id
    JOIN [group] gr ON sg.group_id = gr.group_id
    GROUP BY s.student_id, sg.group_id, gr.year
) AS sa
LEFT JOIN (
    SELECT 
        student_id, 
        average_grade,
        group_id,     
        year,   
        PERCENT_RANK() OVER (PARTITION BY year ORDER BY average_grade DESC) AS rank
    FROM (
        SELECT 
            s.student_id,
            AVG(g.points) AS average_grade,
            sg.group_id,
            gr.year
        FROM grades g
        JOIN student s ON g.student_id = s.student_id
        JOIN student_group sg ON s.student_id = sg.student_id
        JOIN [group] gr ON sg.group_id = gr.group_id
        GROUP BY s.student_id, sg.group_id, gr.year
    ) AS StudentAverage
) AS ts 
ON sa.student_id = ts.student_id AND sa.year = ts.year;</property>
